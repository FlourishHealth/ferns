{
  "name": "ferns-ui-demo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider expo start",
    "android": "NODE_OPTIONS=--openssl-legacy-provider expo start --android",
    "ios": "NODE_OPTIONS=--openssl-legacy-provider expo start --ios",
    "web": "NODE_OPTIONS=--openssl-legacy-provider expo start --web",
    "build-preview": "NODE_OPTIONS=--openssl-legacy-provider yarn build-web",
    "build-web": "NODE_OPTIONS=--openssl-legacy-provider npx expo export:web",
    "eject": "npx expo eject",
    "lint": "eslint \"**/*.ts*\"",
    "lintfix": "eslint --fix \"**/*.ts*\""
  },
  "license": "Apache-2.0",
  "eslintConfig": {
    "extends": [
      "plugin:ferns/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json",
      "tsconfigRootDir": "./"
    },
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "useTabs": false
  },
  "dependencies": {
    "react": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@expo/config-plugins": "~7.8.4",
    "@expo/webpack-config": "^19.0.0",
    "@shopify/flash-list": "1.4.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-ferns": "^0.5.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-comment-length": "^1.7.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-react-perf": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "expo": "^49.0.21",
    "expo-status-bar": "~1.6.0",
    "expo-yarn-workspaces": "~2.2.0",
    "postcss": "8.4.33",
    "prettier": "^3.1.1",
    "react-keyed-flatten-children": "^3.0.0",
    "react-native-haptic-feedback": "^2.0.3",
    "react-native-hyperlink": "^0.0.22",
    "react-native-safe-area-context": "4.6.3",
    "react-native-swiper-flatlist": "^3.2.3",
    "react-native-web": "~0.19.9",
    "swiper": "^11.0.1",
    "typescript": "^5.3.3"
  },
  "engine": {
    "node": "18"
  }
}
