{
  "name": "ferns-ui",
  "version": "0.44.2",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "local": "tsc-watch --onSuccess \"cp -a dist/* ../flourish/node_modules/ferns-ui/dist\"",
    "lint": "eslint \"src/**/*.ts*\"",
    "lintfix": "eslint --fix \"src/**/*.ts*\"",
    "build": "tsc",
    "dev": "tsc -w",
    "deps": "node checkDeps.js"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "useTabs": false
  },
  "eslintConfig": {
    "extends": [
      "plugin:ferns/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json",
      "tsconfigRootDir": "./"
    },
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "modulePaths": [],
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "devDependencies": {
    "@expo/config-plugins": "~7.8.0",
    "@types/mdurl": "^1.0.4",
    "@types/react": "~18.2.57",
    "@types/react-datetime-picker": "^5.0.0",
    "@types/react-signature-canvas": "^1.0.5",
    "@types/react-time-picker": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-comment-length": "^1.7.3",
    "eslint-plugin-ferns": "^0.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "3.1.0",
    "mdurl": "^2.0.0",
    "prettier": "^3.2.5",
    "react": "18.2.0",
    "react-keyed-flatten-children": "^3.0.0",
    "react-router": "^6.22.1",
    "react-router-dom": "^6.22.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-router": "^6.17.0",
    "react-router-dom": "^6.17.0"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.22.3",
    "@react-native-community/blur": "^4.4.0",
    "@react-native-community/datetimepicker": "7.6.1",
    "@react-native-picker/picker": "2.6.1",
    "dayjs": "^1.11.10",
    "expo-clipboard": "~5.0.1",
    "expo-font": "~11.10.3",
    "expo-haptics": "~12.8.1",
    "expo-image-manipulator": "~11.8.0",
    "expo-image-picker": "~14.7.1",
    "expo-localization": "~14.8.3",
    "libphonenumber-js": "^1.10.56",
    "lodash": "^4.17.21",
    "react-app-polyfill": "^3.0.0",
    "react-date-picker": "^10.5.2",
    "react-datetime-picker": "^5.5.3",
    "react-dev-utils": "^12.0.1",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-actions-sheet": "^0.9.2",
    "react-native-calendars": "^1.1301.0",
    "react-native-drawer-layout": "3.2.2",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-google-places-autocomplete": "^2.5.6",
    "react-native-modalize": "^2.1.1",
    "react-native-permissions": "^4.1.2",
    "react-native-picker-select": "^9.0.1",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "~3.6.2",
    "react-native-signature-canvas": "^4.7.1",
    "react-native-svg": "14.1.0",
    "react-native-swiper-flatlist": "^3.2.3",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-webview": "13.6.4",
    "react-signature-canvas": "^1.0.6",
    "react-time-picker": "^6.5.2"
  }
}
